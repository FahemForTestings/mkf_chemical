{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 3,
	"templateParts": [
		{
			"name": "header",
			"title": "Header",
			"area": "header"
			
		},
		{
			"name": "footer",
			"title": "Footer",
			"area": "footer"
		}
	],
	"settings": {
		"appearanceTools": true,
		"layout": {
			"contentSize": "1200px",
			"wideSize": "1340px"
		},
		"color": {
			"defaultPalette": false,
			"palette": [
				{
					"color": "#ffffff",
					"name": "White",
					"slug": "white"
				},
				{
					"color": "#112d44",
					"name": "Primary Dark Blue",
					"slug": "primary_dark_blue"
				},
				{
					"color": "#44a6c3",
					"name": "Primary Cyan",
					"slug": "primary_cyan"
				},
				{
					"color": "#808080",
					"name": "Gray",
					"slug": "gray"
				},
				{
					"color": "#e4f2f4",
					"name": "Secondary Gray",
					"slug": "secondary_gray"
				},
				{
					"color": "#f2f2f2",
					"name": "Secondary Gray Alt",
					"slug": "secondary_gray_alt"
				},
				{
					"color": "#254a66",
					"name": "Secondary Dark Blue",
					"slug": "secondary_dark_blue"
				},
				{
					"color": "#1a1a1a",
					"name": "Secondary Black",
					"slug": "secondary_black"
				},
				{
					"color": "#182730",
					"name": "Secondary Black Alt",
					"slug": "secondary_black_alt"
				}
			]
		},
		"typography": {
			"fontFamilies": [
				{
					"fontFamily": "'Poppins', sans-serif",
					"slug": "poppins",
					"name": "Poppins",
					"fontFace": [
						{
							"fontFamily": "Poppins",
							"fontStyle": "thin",
							"fontWeight": 100,
							"src": "[file:fonts/Poppins-Thin.ttf]"
						},
						{
							"fontFamily": "Poppins",
							"fontStyle": "normal",
							"fontWeight": 400,
							"src": "[file:fonts/Poppins-Regular.ttf]"
						},
						{
							"fontFamily": "Poppins",
							"fontStyle": "Bold",
							"fontWeight": 700,
							"src": "[file:fonts/Poppins-Bold.ttf]"
						}
					]
				}
			],
			"fontSizes": [
				{
					"size": "0.75rem",
					"name": "Extra Small",
					"slug": "extra_small"
				},
				{
					"size": "0.875rem",
					"name": "Small",
					"slug": "small"
				},
				{
					"size": "1rem",
					"name": "Regular",
					"slug": "regular"
				},
				{
					"size": "1.25rem",
					"name": "Large",
					"slug": "large"
				},
				{
					"size": "1.5rem",
					"name": "Extra Large",
					"slug": "extra_large"
				},
				{
					"size": "2rem",
					"name": "Huge",
					"slug": "huge"
				},
				{
					"size": "2.5rem",
					"name": "Giant",
					"slug": "giant"
				}
			]
		},
		"spacing": {
			"padding": true,
			"margin": true,
			"blockGap": true,
			"units": ["px","em","rem","vh","vw","%"]
		}
	},
	"styles": {
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--poppins)",
			"fontSize": "1rem",
			"lineHeight": "1.5"
		},
		"elements": {
			"h1": {
				"typography": {
					"fontSize": "2.5rem",
					"fontWeight": "700"
				},
				"color": {
					"text": "var(--wp--preset--color--primary-cyan)"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "2rem",
					"fontWeight": "700"
				},
				"color": {
					"text": "var(--wp--preset--color--primary-cyan)"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "1.5rem",
					"fontWeight": "700"
				},
				"color": {
					"text": "var(--wp--preset--color--primary-cyan)"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "1.25rem",
					"fontWeight": "700"
				},
				"color": {
					"text": "var(--wp--preset--color--primary-cyan)"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "1rem",
					"fontWeight": "700"
				},
				"color": {
					"text": "var(--wp--preset--color--primary-cyan)"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "0.875rem",
					"fontWeight": "700"
				},
				"color": {
					"text": "var(--wp--preset--color--primary-cyan)"
				}
			}
		},
		"blocks": {
			"core/button": {
				"typography": {
					"fontSize": "1rem",
					"fontWeight": "700"
				},
				"color": {
					"text": "var(--wp--preset--color--white)",
					"background": "var(--wp--preset--color--primary-dark-blue)"
				},
				"border": {
					"radius": "0.25rem"
				},
				"spacing": {
					"padding": {
						"top": ".5rem",
						"right": "1rem",
						"bottom": ".5rem",
						"left": "1rem"
					},
					"margin": {
						"top": "1rem",
						"right": "0",
						"bottom": "1rem",
						"left": "0"
					}
				}
			}
		}
	}
	
}